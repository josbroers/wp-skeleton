{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 2,
  "settings": {
    "custom": {
      "borderRadius": "calc(1rem / 3)",
      "transitionDuration": "250ms",
      "transitionTimingFunction": "ease"
    },
    "color": {
      "palette": [
        {
          "name": "Black",
          "slug": "black",
          "color": "#000000"
        },
        {
          "name": "White",
          "slug": "white",
          "color": "#FFFFFF"
        },
        {
          "name": "Blue",
          "slug": "blue",
          "color": "#0070F3"
        }
      ]
    },
    "typography": {
      "fontFamilies": [
        {
          "fontFamily": "Inter, -apple-system, BlinkMacSystemFont, avenir next, avenir, segoe ui, helvetica neue, helvetica, Ubuntu, roboto, noto, arial, sans-serif",
          "slug": "primary-font",
          "name": "Primary Font"
        }
      ],
      "fontSizes": [
        {
          "slug": "16",
          "size": "1rem",
          "name": "16px"
        }
      ]
    }
  },
  "styles": {
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--primary-font)"
    }
  }
}
